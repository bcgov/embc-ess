{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "annotations": {
            "description": "Defines how to deploy embcess"
        },
        "name": "embcess-develop"
    },
    "objects": [
        {
            "apiVersion": "image.openshift.io/v1",
            "kind": "ImageStream",
            "metadata": {
                "creationTimestamp": null,
                "generation": 1,
                "name": "embcess-develop",
                "selfLink": "/apis/image.openshift.io/v1/namespaces/jhnamn-tools/imagestreams/embcess"
            },
            "spec": {
                "lookupPolicy": {
                    "local": false
                }
            },
            "status": {
                "dockerImageRepository": "docker-registry.default.svc:5000/embcess-develop"
            }
        },
        {
            "apiVersion": "build.openshift.io/v1",
            "kind": "BuildConfig",
            "metadata": {
                "labels": {
                    "app": "embcess-develop"
                },
                "name": "embcess-develop",
                "selfLink": "/apis/build.openshift.io/v1/namespaces/jhnamn-tools/buildconfigs/embcess-develop"
            },
            "spec": {
                "failedBuildsHistoryLimit": 5,
                "nodeSelector": null,
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "embcess-develop:latest"
                    }
                },
                "postCommit": {},
                "resources": {
                    "limits": {
                        "cpu": "1",
                        "memory": "6Gi"
                    },
                    "requests": {
                        "cpu": "1",
                        "memory": "4Gi"
                    }
                },
                "runPolicy": "Serial",
                "source": {
                    "git": {
                        "ref": "develop",
                        "uri": "https://github.com/bcgov/embc-ess.git"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "env": [
                            {
                                "name": "DOTNET_STARTUP_PROJECT",
                                "value": "embc-app/embc-app.csproj"
                            },
                            {
                                "name": "UAT_BUILD"
                            },
                            {
                                "name": "PROD_BUILD"
                            }
                        ],
                        "from": {
                            "kind": "DockerImage",
                            "name": "registry.access.redhat.com/dotnet/dotnet-21-rhel7:latest"
                        }
                    },
                    "type": "Source"
                },
                "successfulBuildsHistoryLimit": 5,
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "github": {
                            "secretReference": {
                                "name": "embcess-build-secret"
                            }
                        },
                        "type": "GitHub"
                    }
                ]
            },
            "status": {
            }
        }
    ]
}


